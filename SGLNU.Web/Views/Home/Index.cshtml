<section class="contact section container" id="contact">
    <div class="contact__container grid">
        <div class="contact__content">
            <h2 class="section__title-center">Виникли питання? <br> Залиш свої тут</h2>
            <div class="forms__container">
                <form method="post" role="form" asp-action="SendQuestion" asp-controller="Home"
                      data-ajax-success="Bindgrid"
                      data-ajax="true"
                      data-ajax-method="POST">
                    <div class="row">
                        <div class="col-25">
                            <h4 class="contact__text" for="fname">Ім'я та Прізвище</h4>
                        </div>
                        <div class="col-75">
                            <input required="required" asp-for="@Model.Name" type="text" id="fname">
                            <span hidden asp-validation-for="@Model.Name" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-25">
                            <h4 class="contact__text" for="lname">E-mail</h4>
                        </div>
                        <div class="col-75">
                            <input type="text" id="email" required="required" asp-for="@Model.Email">
                            <span hidden asp-validation-for="@Model.Email" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-25">
                            <h4 class="contact__text" for="subject">Запитання</h4>
                        </div>
                        <div class="col-75">
                            <textarea id="message" asp-for="@Model.FeedBackDescription" type="text" required="required" style="height:200px"></textarea>
                            <span hidden asp-validation-for="@Model.FeedBackDescription" class="text-danger"></span>
                        </div>
                    </div>
